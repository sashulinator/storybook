{
  "name": "storybook",
  "packageManager": "yarn@3.3.0",
  "version": "0.0.0",
  "description": "Best template",
  "homepage": "https://gitlab.inno.tech/colibri/rtm-client-ui",
  "author": "Savchenko Alexander",
  "license": "UNLICENSED",
  "scripts": {
    "generate:mocks": "yarn ts-node -O {\\\"module\\\":\\\"commonjs\\\"} -T scripts/generate-static-mocks",
    "module:diff": "yarn ts-node -O {\\\"module\\\":\\\"commonjs\\\"} -T scripts/show-modules-diff-stat",
    "module:add-remotes": "yarn ts-node -O {\\\"module\\\":\\\"commonjs\\\"} -T scripts/add-remotes",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "test": "jest",
    "postinstall": "husky install && yarn run module:add-remotes",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@atlaskit/tree": "8.8.6",
    "@dnd-kit/core": "6.0.8",
    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.1",
    "@react-spring/web": "9.7.3",
    "@use-gesture/react": "10.2.27",
    "color": "4.2.3",
    "color-transitions": "0.0.2",
    "d3": "7.8.5",
    "dayjs": "1.11.9",
    "dom-align-ts": "0.0.10",
    "jest": "29.6.4",
    "mitt": "3.0.0",
    "omit-empty-es": "1.2.0",
    "qs": "6.10.3",
    "react": "18.2.0",
    "react-custom-scrollbars": "4.2.1",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.4",
    "react-router-dom": "6.8.0",
    "react-spinners": "0.13.8"
  },
  "devDependencies": {
    "@testing-library/react": "14.0.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/color": "3.0.3",
    "@types/d3": "7.4.0",
    "@types/jest": "29.5.3",
    "@types/qs": "6.9.7",
    "@types/react": "18.2.20",
    "@types/react-custom-scrollbars": "4.0.10",
    "@types/react-dom": "18.2.7",
    "@types/react-mentions": "4.1.8",
    "@types/recoil": "0.0.9",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "6.0.0",
    "husky": "8.0.3",
    "jest-environment-jsdom": "29.6.2",
    "lint-staged": "10.5.4",
    "pinst": "3.0.0",
    "prettier": "3.1.0",
    "sass": "1.66.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vite-plugin-checker": "0.6.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
